import math


potega = []
for i in range(1,15):
    potega.append(i**5)
print(potega)

silnia =[]
for a in range(1,21):
    int = 1
    for y in range(1,a):
        int = int*y
        silnia.append(int)
print(silnia)

e = []
for i in range(0,20):
    e.append(math.exp(i))
print(e)

nazwiska = ["wojak","perla","harnas","kaszlak","okocim"]
licz = []
for i in range(0,5):
    licz.append(len(nazwiska[i]))
print(nazwiska)
print(licz)
